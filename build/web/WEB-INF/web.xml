<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>application</servlet-name>
        <servlet-class>cloud.application</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>app_store</servlet-name>
        <servlet-class>cloud.app_store</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>final_allot</servlet-name>
        <servlet-class>cloud.final_allot</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>feedback</servlet-name>
        <servlet-class>cloud.feedback</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>cloud.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>change_password</servlet-name>
        <servlet-class>cloud.change_password</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_item</servlet-name>
        <servlet-class>cloud.add_item</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>item_to_stock</servlet-name>
        <servlet-class>cloud.item_to_stock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>count</servlet-name>
        <servlet-class>cloud.count</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>selection</servlet-name>
        <servlet-class>cloud.selection</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>room_feedback</servlet-name>
        <servlet-class>cloud.room_feedback</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>application</servlet-name>
        <url-pattern>/application</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>app_store</servlet-name>
        <url-pattern>/app_store</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>final_allot</servlet-name>
        <url-pattern>/final_allot</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>feedback</servlet-name>
        <url-pattern>/feedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>change_password</servlet-name>
        <url-pattern>/change_password</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_item</servlet-name>
        <url-pattern>/add_item</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>item_to_stock</servlet-name>
        <url-pattern>/item_to_stock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>count</servlet-name>
        <url-pattern>/count</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>selection</servlet-name>
        <url-pattern>/selection</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>room_feedback</servlet-name>
        <url-pattern>/room_feedback</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
